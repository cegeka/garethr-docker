description "start and stop <%= title %> in docker"
author "Gareth Rushgrove"

start on runlevel [2345]
stop on runlevel [!2345]

setuid root

respawn
respawn limit 5 20

exec docker run<% if ports %><% ports_array.each do |port| %> -p <%= port %><% end %><% end %> -m <%= memory_limit %> <%= image %> <%= command %>
